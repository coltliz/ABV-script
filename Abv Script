-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("$moke ABV script", 5013109572)

-- themes
local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
}

-- first page
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("Basic")
local section2 = page:addSection("Buying")
local section3 = page:addSection("Farming")
local section4 = page:addSection("Money")

-- Notification
venyx:Notify("Notification", "This script was made by $mokeGoodMids#0001 :D", function()
end)

section1:addButton("Use Arrow", function()
local plyr = game.Players.LocalPlayer
plyr.Character.Humanoid:EquipTool(plyr.Backpack:findFirstChild('Arrow'))
game:GetService("ReplicatedStorage").Arrow:FireServer()
end)

section1:addButton("Use Roka", function()
local plyr = game.Players.LocalPlayer
plyr.Character.Humanoid:EquipTool(plyr.Backpack:findFirstChild('Rokakaka Fruit'))
game:GetService("ReplicatedStorage").Roka:FireServer()
end)
section2:addButton("Buy Arrow", function()
	game:GetService("ReplicatedStorage").BuyArrow:FireServer()
end)
section2:addButton("Buy Roka", function()
	game:GetService("ReplicatedStorage").BuyRoka:FireServer()
end)
section3:addButton("Item Farm", function()
	_G.on = true --false to turn off and true to turn on
while _G.on do
wait(1) -- you can change the delay here
char = game:GetService('Players').LocalPlayer.Character
game:GetService('Players').LocalPlayer.CharacterAdded:connect(function(body)
char = body end)
for i,v in pairs(workspace:GetDescendants()) do
if v:IsA('Tool') then
char.Humanoid:EquipTool(v) end end
end
end)
section4:addButton("Auto use money", function()
local plyr = game.Players.LocalPlayer
plyr.Character.Humanoid:EquipTool(plyr.Backpack:findFirstChild('Banknote'))
end)

    
---misc
local Misc = venyx:addPage("Miscellaneous", 5012544693)
local section1 = Misc:addSection("Misc")
section1:addButton("Rejoin", function()
game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
end)
section1:addButton("SPTW Effect(NEED SPTW)", function()
local args = {[1] = game:GetService("Players").LocalPlayer.Character.Stand.effeb.effectt,[2] = true}
game:GetService("ReplicatedStorage").Trail:FireServer(unpack(args))
end)




---second page ykyk
local anims = venyx:addPage("Animations", 5012544693)
local section1 = anims:addSection("Animations")

section1:addButton("Standless Animation", function()
   while wait() do
   game:GetService("ReplicatedStorage").StandlessAnim:FireServer()
   end
end)

---teleports
local Tp = venyx:addPage("Teleports", 5012544693)
local section1 = Tp:addSection("Teleports")
section1:addButton("Stand Storage", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-152.359, 1076.76, -309.288)
end)
section1:addButton("P.K.V", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-76.5955, 1077.64, -409.845)
end)
section1:addButton("Gyro(spin)", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-346.406, 1096.51, -308.958)
end)
section1:addButton("Sakuya(watch)", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-219.587, 1114.69, -427.225)
end)
section1:addButton("Mansion(Dio)", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(161.353, 1087.04, -316.02)
end)
section1:addButton("Just(Arrow and roka shop)", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-48.1526, 1077.18, -259.282)
end)

--thrid page
local theme = venyx:addPage("Theme", 5012544693)
local colors = theme:addSection("Colors")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
	colors:addColorPicker(theme, color, function(color3)
		venyx:setTheme(theme, color3)
	end)
end


